{
  "name": "chat",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "tsc": "tsc",
    "preview": "vite preview",
    "prettier": "prettier --write ./src",
    "prepare": "husky install",
    "test": "NODE_ENV=test vitest run --coverage"
  },
  "lint-staged": {
    "src/**/*": [
      "prettier --write"
    ],
    "src/**/*.ts?(x)": [
      "tsc-files --noEmit"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.27.2",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.0",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.27.2"
  }
}
